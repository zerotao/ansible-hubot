{
  "name": "{{ robot_name }}",
  "version": "0.0.0",
  "private": true,
  "author": "{{ robot_author }}",
  "description": "{{ robot_desc }}",
  "dependencies": {
  {% for key, value in robot_scripts.iteritems() %}
  {% if not loop.last %}
  "{{ key}}": "{{ value }}",
  {% else %}
  "{{ key }}": "{{ value }}"
  {% endif %}
{% endfor %}
  },
  "engines": {
    "node": "0.10.x"
  }
}
